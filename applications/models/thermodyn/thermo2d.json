// -*- mode: javascript -*-
{
    "Name": "Thermo dynamics",
    "ShortName":"ThermoDyn",
    "Model":"mythermicmodel",
    // "Parameters":
    // {
    //     "gravity":
    //     {
    //         "value":"-1"
    //     }
    // },
    "Materials":
    {
        "beam":{ "name":"Copper","filename":"$feelpp_srcdir/databases/materials/pure/copper.json" }
    },
    "BoundaryConditions":
    {
        "temperature":
        {
            "Dirichlet":
            {
                "MarkerDirichlet":
                {
                    "expr":"293-10*(y-1)*(y+1):y"
                }
            },
            "Dirichlet":
            {
                "wall":
                {
                    "expr":"293+0.38*t:t"
                }
            },
            "Neumann":
            {
                "MarkerNeumann":
                {
                    "expr":"0:x" 
                }
            }
        }
    },
    "PostProcess":
    {
        "Fields":["temperature"]
    }

}
